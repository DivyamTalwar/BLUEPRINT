
# API Keys - Required for BLUEPRINT

# OpenRouter API Key (for Claude 3.5 and 3.7 Sonnet)
# Get yours at: https://openrouter.ai/
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Cohere API Key (for embeddings)
# Get yours at: https://cohere.com/
COHERE_API_KEY=your_cohere_api_key_here

# Pinecone API Key (for vector database)
# Get yours at: https://www.pinecone.io/
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=your_pinecone_environment

# Development Settings
DEBUG=False
LOG_LEVEL=INFO

# NOTE: Gemini has been removed due to overly aggressive safety filters
# (69% failure rate on innocent programming terms like "authentication", "REST API")
# The system now uses Claude 3.5 Sonnet (simple tasks) and Claude 3.7 Sonnet (complex tasks)
